{
  "expo": {
    "name": "TestDownload2",
    "slug": "testdownload2",
	"owner": "onehat",
    "version": "1.0.0",
	"privacy": "unlisted",
	"jsEngine": "hermes",
	"runtimeVersion": {
		"policy": "sdkVersion"
	},
	"platforms": [
		"ios",
		"android",
		"web"
	],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
		"supportsTablet": true,
		"requireFullScreen": true,
		"isTabletOnly": false,
		"usesIcloudStorage": false,
		"usesAppleSignIn": false,
		"accessesContactNotes": false,
		"infoPlist": {
			"UIBackgroundModes": [
				"audio"
			]
		}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
	  "package": "com.testdownload2",
	  "versionCode": 3,
	  "permissions": [
		  "READ_EXTERNAL_STORAGE",
		  "WRITE_EXTERNAL_STORAGE",
		  "INTERNET"
	  ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "extra": {
	"eas": {
	  "projectId": "4dae9d64-43c5-44b7-9a8f-312f292ce938"
	}
  }
}
